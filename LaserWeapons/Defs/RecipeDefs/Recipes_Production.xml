<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    
    <RecipeDef>
        <defName>Make_LaserCapacitor</defName>
        <label>make laser capacitor</label>
        <description>Make a laser capacitor.</description>
        <jobString>Making laser capacitor.</jobString>
        <workSpeedStat>SmithingSpeed</workSpeedStat>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_Machining</soundWorking>
        <workAmount>4000</workAmount>
        <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
        <skillRequirements>
            <Crafting>10</Crafting>
        </skillRequirements>
        <researchPrerequisite>Lasers</researchPrerequisite>
        <workSkill>Crafting</workSkill>
        <recipeUsers>
            <li>TableMachining</li>
        </recipeUsers>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>ComponentIndustrial</li>
                    </thingDefs>
                </filter>
                <count>4</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                    </thingDefs>
                </filter>
                <count>20</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Plasteel</li>
                    </thingDefs>
                </filter>
                <count>4</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Gold</li>
                    </thingDefs>
                </filter>
                <count>8</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Silver</li>
                    </thingDefs>
                </filter>
                <count>48</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>ComponentIndustrial</li>
                <li>Steel</li>
                <li>Plasteel</li>
                <li>Silver</li>
                <li>Gold</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <ComponentIndustrial>4</ComponentIndustrial>
        </products>
    </RecipeDef>
    
    
